    <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
    <script src="parse-1.2.8.min.js"></script>
    <script>
    $.load(function () {
        Parse.initialize("aP5gU0Rr5ztADm6tTIxXubw7Ya78vCNuMKYzE6T0", "3INKWhRf8VvdXhnhj3GdGP0QPik8Zt9YGyhqYuHG");
        var user_query = new Parse.Query(User);
        user_query.equalTo("username", "diegofuentesm@hotmail.com");
        settings_query.first({
            success: function(object) {
                // Successfully retrieved the object.
                console.log(object);
                var settings_query = new Parse.Query(Feed);
                settings_query.equalTo("user", Parse.User.current());
                Settings_query.equalTo("service","settings");
                settings_query.first({
                    success: function(object) {
                        // Successfully retrieved the object.
                        console.log(object);
                        var settings_map = object.get('extra');
                        $.each(settings_map, function(setting, state) {
                            if (state===true){
                                $('body').addClass(setting);
                                console.log('enabling'+setting);
                            }
                        });
                    },
                    error: function(error) {
                        alert("Error: " + error.code + " " + error.message);
                    }
                });

            });
                            },
        error: function(error) {
            alert("Error: " + error.code + " " + error.message);
        }
        });
    });
    </script>
