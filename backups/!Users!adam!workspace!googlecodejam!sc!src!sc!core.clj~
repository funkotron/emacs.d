(ns sc.core)

(defn input
  "Read in the test cases"
  [x]
  (clojure.string/split (slurp x) #"\n"))

(defn tcs [] (partition 3 (rest (input "A-small-practice.in"))))

(defn addsup [i others] (
                         (filter (cond))))

(defn check [tc]
  (let [
        cash (first tc)
        nitems (nth tc 1)
        prices (nth tc 2)]
    (println cash nitems prices)
    ))

(map check (tcs))

