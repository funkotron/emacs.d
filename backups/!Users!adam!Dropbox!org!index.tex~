\label{sec-2-2-6-1-4}%
\texttt{CLOSED:} \textit{2012-12-05 Wed 18:25}

\begin{itemize}

\item Use acpi$_{\mathrm{listen}}$ to find the arguments (given as \$1 \$2 ..)
\label{sec-2-2-6-1-4-1}%

\item Create /etc/acpi/events/anything\\
\label{sec-2-2-6-1-4-2}%
event=button/lid
action=/etc/acpi/handler.sh \%e

\item Create /etc/acpi/handler.sh\\
\label{sec-2-2-6-1-4-3}%
LIDSTATEFILE=''/home/adam/etc/lidclosedstate''
echo ``HOWDY'' \$1 \$2 \$3 \$4 >> /home/adam/acpi.log;
if [ ``\$2'' = LID0 ]
then
    ISCLOSED=`cat \$LIDSTATEFILE`
    if [ ``\$ISCLOSED'' = 1 ]
    then
        echo 0 > \$LIDSTATEFILE
    else
        \#echo -n mem >/sys/power/state
        echo 1 > \$LIDSTATEFILE
        /usr/sbin/pm-suspend
    fi
fi
\end{itemize} % ends low level

